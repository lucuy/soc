<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="threshold" extends="struts-default" namespace="/threshold">

		<!--  阀值修改 -->
		<action name="thresholdEdit" class="thresholdAction" method="editThreshold">
			<result name="success">/pages/alert/threshold.jsp</result>
		</action>
		
		<!--  阀值更新 -->
		<action name="updateThreshold" class="thresholdAction" method="updateThreshold">
			<result name="success">/pages/alert/threshold.jsp</result>
		</action>
		
	</package>
</struts>