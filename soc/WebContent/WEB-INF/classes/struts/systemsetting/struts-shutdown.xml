<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="settingShutdown" extends="struts-default" namespace="/settingShutdown">
		<action name="shutdownServer" class="SettingShutdownAction" method="shutdownServer">
			<result name="success">/pages/systemsetting/setting_shutdown.jsp</result>
		</action>		
		<action name="rebootServer" class="SettingShutdownAction" method="rebootServer">
			<result name="success">/pages/systemsetting/setting_shutdown.jsp</result>     	
		</action>		
	</package>
</struts>