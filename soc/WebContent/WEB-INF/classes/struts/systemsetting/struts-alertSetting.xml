<struts>
	<package name="alertSetting" extends="struts-default" namespace="/alertSetting">

		<!-- 告警配置查询 -->
		<action name="queryAlert" class="SettingAlertAction" method="queryAlert">
			<result name="success">/pages/alert/alert_setting.jsp</result>
		</action>
